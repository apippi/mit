"  Width 3;\n"
"  Height 1;\n"
"\n"
"  ColExpand true;\n"
"  ColAlign Center;\n"
"\n"
"  RowAlign Center;\n"
"\n"
"  Color ScreenHeaderColor;\n"
"  ColumnsCfg HEADER;\n"
"};"

