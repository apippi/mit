#, c-format
msgid "%ls: Examine"
msgstr "%ls: Prüfen"

#: src/broken_indicator.cc:137
#, c-format
msgid "%ls: Apply"
msgstr "%ls: Anwenden"

#: src/broken_indicator.cc:142
#, c-format
msgid "%ls: Next"
msgstr "%ls: Nächste"

#: src/broken_indicator.cc:148
#, c-format
